<Window x:Class="dubbelsteen_h4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dubbelsteen_h4"
        mc:Ignorable="d"
        Title="MainWindow" Height="303" Width="558">
    <Grid Background="#FFEE6AB8" Margin="0,0,0,-6">
        <Button x:Name="startButton" Content="Start" HorizontalAlignment="Left" Margin="348,62,0,0" VerticalAlignment="Top" Background="#FFF1F1F1" FontSize="16" Foreground="#FF791212" Height="30" Width="110" Click="startButton_Click"/>
        <Button x:Name="againButton" Content="Opnieuw" HorizontalAlignment="Left" Margin="348,0,0,0" VerticalAlignment="Center" Background="#FFF1F1F1" FontSize="16" Foreground="#FF791212" Height="30" Width="110" Click="againButton_Click"/>
        <Button x:Name="closeButton" Content="Sluiten" HorizontalAlignment="Left" Margin="348,191,0,0" VerticalAlignment="Top" Background="#FFF1F1F1" FontSize="16" Foreground="#FF791212" Height="30" Width="110" Click="closeButton_Click"/>
        <TextBox x:Name="resultTextBox" HorizontalAlignment="Left" Margin="10,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="292" Height="199" FontSize="14"/>

    </Grid>
</Window>
